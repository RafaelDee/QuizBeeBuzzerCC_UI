{
  "name": "buzzer-control-center",
  "productName": "buzzer-control-center",
  "version": "0.0.0",
  "description": "Buzzer Control Center",
  "author": {
    "name": "Rafael Dee"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build-buzzer-control-center": "ng build",
    "serve-buzzer-control-center": "ng serve BuzzerControlCenter",
    "start-electron": "ng build && electron .",
    "build-electron": "tsc --project projects/electron/tsconfig.json",
    "wait-for-serve-buzzer-control-center": "wait-on http://localhost:4200",
    "watch": "ng build --watch --configuration development",
    "start": "concurrently \"npm run build-electron\" \"npm run serve-buzzer-control-center\" \"npm run wait-for-serve-buzzer-control-center && electron-forge start\"",
    "test": "ng test",
    "package": "npm run build-electron && npm run build-buzzer-control-center && electron-forge package",
    "make": "npm run build-electron && npm run build-buzzer-control-center && electron-forge make",
    "publish-app": "npm run build-electron && npm run build-buzzer-control-center && electron-forge publish"
  },
  "main": "dist/index.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "@types/w3c-web-serial": "^1.0.7",
    "@types/web": "^0.0.198",
    "bootstrap": "^5.3.3",
    "browser-serial": "^1.0.7",
    "electron-squirrel-startup": "^1.0.1",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "rxjs": "~7.8.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.0",
    "@angular/localize": "^19.1.0",
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "@types/jasmine": "~5.1.0",
    "electron": "^34.1.1",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
